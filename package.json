{
  "name": "capricorn-ui",
  "title": "Capricorn UI",
  "keywords": [
    "capricorn-ui",
    "component",
    "components",
    "frontend",
    "react",
    "react-component",
    "ui"
  ],
  "module": "esm/index.js",
  "version": "0.14.2",
  "main": "lib/index.js",
  "unpkg": "dist/capricorn-ui.js",
  "typings": "lib/index.d.ts",
  "license": "MIT",
  "sideEffects": [
    "dist/*",
    "*.css"
  ],
  "scripts": {
    "build": "webpack -c build.config.js",
    "build:esm": "tsc -p tsconfig.esm.json && yarn css:esm && tsc-alias -p tsconfig.esm.json",
    "build:lib": "tsc && yarn css:lib && tsc-alias",
    "css:lib": "rsync -R components/**/*.css lib && cd lib && rsync -a components/ ./ && rm -rf components",
    "css:esm": "rsync -R components/**/*.css esm && cd esm && rsync -a components/ ./ && rm -rf components",
    "sb": "start-storybook -p 6006",
    "build:sb": "build-storybook",
    "format:css": "csscomb components/**/*.css",
    "format:js": "prettier --write components/**/*.{ts,tsx}",
    "build:all": "yarn build & yarn build:esm & yarn build:lib && yarn css:lib & yarn css:esm"
  },
  "dependencies": {
    "classnames": "^2.2.6",
    "copy-to-clipboard": "^3.3.1",
    "imask": "^6.1.0",
    "react-calendar": "^3.3.0"
  },
  "devDependencies": {
    "@babel/core": "^7.12.10",
    "@babel/plugin-proposal-optional-chaining": "^7.12.7",
    "@babel/plugin-syntax-dynamic-import": "^7.8.3",
    "@babel/plugin-transform-runtime": "^7.12.10",
    "@babel/preset-env": "^7.12.11",
    "@babel/preset-react": "^7.12.10",
    "@babel/preset-typescript": "^7.12.7",
    "@pmmmwh/react-refresh-webpack-plugin": "^0.4.3",
    "@storybook/addon-actions": "^6.1.14",
    "@storybook/addon-essentials": "^6.1.14",
    "@storybook/addon-links": "^6.1.14",
    "@storybook/react": "^6.1.14",
    "@types/classnames": "^2.2.11",
    "@types/node": "^14.14.14",
    "@typescript-eslint/eslint-plugin": "^4.10.0",
    "@typescript-eslint/parser": "^4.10.0",
    "babel-loader": "^8.2.2",
    "clean-webpack-plugin": "^3.0.0",
    "css-loader": "^5.0.1",
    "csscomb": "^4.3.0",
    "eslint": "^7.15.0",
    "eslint-config-prettier": "^7.0.0",
    "eslint-plugin-optimize-regex": "^1.2.0",
    "eslint-plugin-prettier": "^3.3.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-react": "^7.21.5",
    "eslint-plugin-react-hooks": "^4.2.0",
    "eslint-plugin-sonarjs": "^0.5.0",
    "eslint-plugin-sort-imports-es6-autofix": "^0.5.0",
    "file-loader": "^6.2.0",
    "html-webpack-plugin": "^5.0.0-alpha.17",
    "less-loader": "^7.1.0",
    "mini-css-extract-plugin": "^1.3.3",
    "optimize-css-assets-webpack-plugin": "^5.0.4",
    "prettier": "^2.2.1",
    "raw-loader": "^4.0.2",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-svg-loader": "^3.0.3",
    "react-use": "^15.3.4",
    "storybook": "^6.1.14",
    "storybook-addon-react-docgen": "^1.2.42",
    "storybook-addon-themes": "^6.0.1",
    "style-loader": "^2.0.0",
    "svg-inline-loader": "^0.8.2",
    "svg-url-loader": "^7.1.1",
    "terser-webpack-plugin": "^5.0.3",
    "typescript": "^4.1.3",
    "url-loader": "^4.1.1",
    "webpack": "^5.10.3",
    "webpack-cli": "^4.2.0",
    "webpack-dev-server": "^3.11.0"
  },
  "peerDependencies": {
    "react": ">=16.9.0",
    "react-dom": ">=16.9.0"
  },
  "files": [
    "assets",
    "theme",
    "constants",
    "dist",
    "lib",
    "esm"
  ]
}
