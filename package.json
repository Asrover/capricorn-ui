{
  "name": "capricorn-ui",
  "version": "0.0.13",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "build": "webpack -c .config/build.config.js",
    "build:lib-esm": "tsc -m es6 --outDir lib-esm && yarn copy-css:lib-esm",
    "build:lib": "tsc && yarn copy-css:lib",
    "sb": "start-storybook -p 6006",
    "build-storybook": "build-storybook",
    "format:css": "csscomb src/components/**/*.css",
    "format:js": "prettier --write src/**/*.{ts,tsx}",
    "copy-css:lib": "cp src/components/**/*.css lib/components/** & mkdir lib/theme & cp src/theme/*.css lib/theme",
    "copy-css:lib-esm": "cp src/components/**/*.css lib-esm/components/** & mkdir lib-esm/theme & cp src/theme/*.css lib-esm/theme",
    "build:all": "yarn build & yarn build:lib-esm & yarn build:lib"
  },
  "dependencies": {
    "classnames": "^2.2.6",
    "copy-to-clipboard": "^3.3.1"
  },
  "devDependencies": {
    "@babel/core": "^7.12.10",
    "@babel/plugin-proposal-optional-chaining": "^7.12.7",
    "@babel/plugin-syntax-dynamic-import": "^7.8.3",
    "@babel/plugin-transform-runtime": "^7.12.10",
    "@babel/preset-env": "^7.12.11",
    "@babel/preset-react": "^7.12.10",
    "@babel/preset-typescript": "^7.12.7",
    "@pmmmwh/react-refresh-webpack-plugin": "^0.4.3",
    "@storybook/addon-actions": "^6.1.14",
    "@storybook/addon-essentials": "^6.1.14",
    "@storybook/addon-links": "^6.1.14",
    "@storybook/react": "^6.1.14",
    "@types/classnames": "^2.2.11",
    "@types/node": "^14.14.14",
    "@typescript-eslint/eslint-plugin": "^4.10.0",
    "@typescript-eslint/parser": "^4.10.0",
    "babel-loader": "^8.2.2",
    "clean-webpack-plugin": "^3.0.0",
    "csscomb": "^4.3.0",
    "eslint": "^7.15.0",
    "eslint-config-prettier": "^7.0.0",
    "eslint-plugin-optimize-regex": "^1.2.0",
    "eslint-plugin-prettier": "^3.3.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-react": "^7.21.5",
    "eslint-plugin-react-hooks": "^4.2.0",
    "eslint-plugin-sonarjs": "^0.5.0",
    "eslint-plugin-sort-imports-es6-autofix": "^0.5.0",
    "file-loader": "^6.2.0",
    "html-webpack-plugin": "^5.0.0-alpha.17",
    "inputmask": "^5.0.5",
    "less": "^4.0.0",
    "less-loader": "^7.1.0",
    "mini-css-extract-plugin": "^1.3.3",
    "optimize-css-assets-webpack-plugin": "^5.0.4",
    "prettier": "^2.2.1",
    "raw-loader": "^4.0.2",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-dropzone": "^11.2.4",
    "react-router-dom": "^5.2.0",
    "react-svg-loader": "^3.0.3",
    "react-use": "^15.3.4",
    "storybook": "^6.1.14",
    "storybook-addon-react-docgen": "^1.2.42",
    "storybook-addon-themes": "^6.0.1",
    "style-loader": "^2.0.0",
    "svg-inline-loader": "^0.8.2",
    "svg-url-loader": "^7.1.1",
    "terser-webpack-plugin": "^5.0.3",
